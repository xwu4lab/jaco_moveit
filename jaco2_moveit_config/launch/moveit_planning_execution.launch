<launch>
  <rosparam command="load" file="$(find jaco2_moveit_config)/config/joint_names.yaml"/>

  <arg name="sim" default="true" />
  <arg name="robot_ip" unless="$(arg sim)" />

  <include file="$(find jaco2_moveit_config)/launch/planning_context.launch" >
    <arg name="load_robot_description" value="true" />
  </include>


 

  <include file="$(find jaco2_moveit_config)/launch/move_group.launch">
    <arg name="publish_monitored_planning_scene" value="true" />
  </include>

  <include file="$(find jaco2_moveit_config)/launch/jaco_arm.launch">
  </include>
  
  <include file="$(find jaco2_moveit_config)/launch/default_warehouse_db.launch" />

</launch>
